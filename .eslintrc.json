{
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"],
  "parser": "babel-eslint",
  "rules": {
    "max-len": 0,
    "camelcase": 0,
    "no-console": 0,
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "object-curly-newline": 0,
    "no-plusplus": 0,
    "import/prefer-default-export": 0,
    "react/prop-types":0
  },
  "globals": {
    "localStorage": true,
    "sessionStorage": true,
    "fetch": true,
    "window": true,
    "document": true
  }
}